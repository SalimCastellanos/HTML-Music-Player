{
  "name": "html-music-player",
  "version": "1.0.0",
  "description": "HTML music player",
  "main": "index.js",
  "scripts": {
    "build": "gulp css",
    "test": "echo \"Error: no test specified\" && exit 1",
    "watch-player-worker": "NODE_PATH=src watchify src/audio/AudioPlayerBackend.js --standalone AudioPlayer -o dist/worker/AudioPlayerWorker.js",
    "watch-analyzer-worker": "NODE_PATH=src watchify src/audio/TrackAnalyzerBackend.js --standalone TrackAnalyzer -o dist/worker/TrackAnalyzerWorker.js",
    "watch-app": "NODE_PATH=src watchify src/application.js --standalone Application -o dist/main.js",
    "watch-css": "NODE_PATH=src node-sass -w sass/ --recursive sass/app-css-public.scss -o dist/css/",
    "compile-css": "node-sass sass/ --output-style=\"compressed\" --recursive sass/app-css-public.scss -o dist/css/min/app-public-css.css",
    "compile-css-dev": "node-sass sass/ --recursive sass/app-css-public.scss -o dist/css/",
    "start": "npm run compile-css-dev && npm run watch-player-worker | npm run watch-analyzer-worker | npm run watch-app | npm run watch-css | ./https-server"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/petkaantonov/HTML-Music-Player.git"
  },
  "author": "Petka Antonov <petka_antonov@hotmail.com> (https://github.com/petkaantonov)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/petkaantonov/HTML-Music-Player/issues"
  },
  "homepage": "https://github.com/petkaantonov/HTML-Music-Player#readme",
  "devDependencies": {
    "base64-js": "^1.1.1",
    "bluebird": "^3.3.4",
    "del": "^2.2.0",
    "glob": "^7.0.3",
    "gulp": "^3.9.1",
    "gulp-gjslint": "^0.1.5",
    "gulp-rename": "^1.2.2",
    "gulp-replace": "^0.5.4",
    "gulp-sass": "^2.2.0",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-uglify": "^1.5.3",
    "jdenticon": "^1.3.2",
    "js-polyfills": "^0.1.16",
    "mkdirp": "^0.5.1",
    "rollup": "^0.25.4",
    "rollup-plugin-commonjs": "^2.2.1",
    "rollup-plugin-includepaths": "^0.1.1",
    "rollup-plugin-multi-entry": "^1.2.0",
    "rollup-plugin-node-resolve": "^1.5.0",
    "rollup-plugin-uglify": "^0.2.0",
    "rollup-stream": "^1.4.1",
    "ua-parser-js": "^0.7.10",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0"
  }
}
